#9
import math
a = [6,3,8]
temp = 1
for i in range(0, len(a)):
    temp = temp * a[i]
temp2 = (float)(math.pow(temp,(1/len(a))))
res = (float)(temp2)
print("The geometric mean of a is:"+str(res))

