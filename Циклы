#1.1
P = 0
x1 = float(input("Введите первую координату x:"))
y1 = float(input("Введите первую координату y:"))
px = x1
py = y1
a = input("Введите следующую координату x (Enter для окончания ввода):")
while a != "":
    x = float(a)
    y = float(input("Введите следующую коордианту y:"))
    ras = ((px - x)**2 + (py - y)**2)**0.5
    P = P + ras
    px = x
    py = y
    a = input("Введите следующую координату x (Enter для окончания ввода):")
    ras = ((x - x)**2 + (y - y)**2)**0.5
    P = P + ras
print("Периметр многоугольника -", P)

#2.1
#Дети до 2 лет - 0.00
#Дети от 3 до 12 лет - 14.00
#Песнионеры от 65 лет - 18.00
#Взрослый обычный - 23.00
v = input("Введите возраст посетителя: ")
s = 0
while v != "":
    if 0 < int(v) < 2:
        s = s + 0.00
    elif 2 < int(v) < 12:
        s = s + 14.00
    elif 12 < int(v) < 64:
        s = s + 23.00
    else:
        s = s + 18.00
    v = input("Введите возраст посетителя (Enter для окончания ввода): ")
print("Сумма посещения зоопарка для этой группы составит -", s, '$')

#3.1
x = 0
y = 1
print(x, y, end=" ")
k = 2
while 0 < k < 100:
  x, y = y, x + y
  k += 1
  print(y, end=" ")


#4.1
k = 0
while k < 500:
    k += 2
    print(k)

#5.1
chis = 1
for k in range(0, 10000):
 if k % 2 != 0:
     chis *= k
     print(chis)
