#2
for k in range(1, 1 + 10):
    for a in range(1, 11):
        rez = k * a
        print(k, '*', a, '=', rez)
    print(" ")

#3
a1 = int(input("Введите число:"))
a2 = 0

while a1 > 0:
    b = a1 % 10
    a1 = a1 // 10
    a2 = a2 * 10
    a2 = a2 + b
print(a2)

#4
a, b, c = input("Введите трехзначное число:")

if a > b > c:
    print(a + b + c)
if a > c > b:
    print(a + c + b)
if b > a  > c:
    print(a + b + c)
if b > c > a:
    print(b + c + a)
if c > a > b:
    print(c + a + b)
else:
    print(c + b + a)

