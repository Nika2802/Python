#1
import datetime
a = int(input())
b = str(datetime.timedelta(seconds=a))
print(b)

#2
import datetime
a = '2024-10-05 18:48:50'
b = datetime.datetime.strptime(a, '%Y-%m-%d %H:%M:%S')
print(a, type(b))

#3
import datetime
a = datetime.datetime(2024,10,5)
b = datetime.datetime(2023,2,28)
c = a - b
print(c.days)
